    },
    {
      "task_key": "gold_aggregation",
      "depends_on": [{ "task_key": "silver_transformation" }],
      "spark_python_task": {
        "python_file": "/Repos/gold/patient_drug_analytics.py"
      },
      "existing_cluster_id": "cluster-id"
    }
  ]
}
